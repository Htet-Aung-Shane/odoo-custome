				<t t-foreach="docs" t-as="o">
					<div class="page">
						<div class="oe_structure">
							<div class="container">
								<table class="table table-dark">
									<thead>
										<tr>
											<th scope="col">
												#
											</th>
											<th scope="col">
												First
											</th>
											<th scope="col">
												Last
											</th>
											<th scope="col">
												Handle
											</th>
										</tr>
									</thead>
									<tbody>
										<tr>
											<th scope="row">
												1
											</th>
											<td>
												<span t-esc="o.name" />
											</td>
											<td>
												Otto
											</td>
											<td>
												@mdo
											</td>
										</tr>
									</tbody>
								</table>
							</div>
						</div>
					</div>
				</t>
			</t>
		</t>
	</template>
</odoo>
